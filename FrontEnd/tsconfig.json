{
  "compilerOptions": {
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "baseUrl": "",
    "paths": {
      "@/*": ["./*"] 
      
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "FrontEnd/.next/types/**/*.ts",
    "FrontEnd/next-env.d.ts",
    ".next/types/**/*.ts",
    "app/my-account/orders/pending",
    "app/my-account/orders/cancelled",
    "app/my-account/orders/shipped",
    "app/checkout"
  ],
  "exclude": [
    "node_modules"
  ]
}

